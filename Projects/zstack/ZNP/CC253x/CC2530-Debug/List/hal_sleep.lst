    144          /* sleep and external interrupt port masks */
    145          #define STIE_BV                             BV(5)
    146          #define P0IE_BV                             BV(5)
    147          #define P1IE_BV                             BV(4)
    148          #define P2IE_BV                             BV(1)
    149          
    150          /* sleep timer interrupt control */
    151          #define HAL_SLEEP_TIMER_ENABLE_INT()        st(IEN0 |= STIE_BV;)     /* enable sleep timer interrupt */
    152          #define HAL_SLEEP_TIMER_DISABLE_INT()       st(IEN0 &= ~STIE_BV;)    /* disable sleep timer interrupt */
    153          #define HAL_SLEEP_TIMER_CLEAR_INT()         st(STIF = 0;)            /* clear sleep interrupt flag */
    154          
    155          /* backup interrupt enable registers before sleep */
    156          #define HAL_SLEEP_IE_BACKUP_AND_DISABLE(ien0, ien1, ien2) st(ien0  = IEN0;    /* backup IEN0 register */ \
    157                                                                       ien1  = IEN1;    /* backup IEN1 register */ \
    158                                                                       ien2  = IEN2;    /* backup IEN2 register */ \
    159                                                                       IEN0 &= STIE_BV; /* disable IEN0 except STIE */ \
    160                                                                       IEN1 &= P0IE_BV; /* disable IEN1 except P0IE */ \
    161                                                                       IEN2 &= (P1IE_BV|P2IE_BV);) /* disable IEN2 except P1IE, P2IE */
    162          
    163          /* restore interrupt enable registers before sleep */
