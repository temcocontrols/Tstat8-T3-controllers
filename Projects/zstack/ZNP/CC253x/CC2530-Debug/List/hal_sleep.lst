    115          void halSetSleepMode(void);
    116          
    117          /* This value is used to adjust the sleep timer compare value such that the sleep timer
    118           * compare takes into account the amount of processing time spent in function halSleep().
    119           * The first value is determined by measuring the number of sleep timer ticks it from
    120           * the beginning of the function to entering sleep mode or more precisely, when
    121           * MAC_PwrNextTimeout() is called.  The second value is determined by measuring the number
    122           * of sleep timer ticks from exit of sleep mode to the call to MAC_PwrOnReq() where the
    123           * MAC timer is restarted.
    124           */
    125          #define HAL_SLEEP_ADJ_TICKS   (11 + 12)
    126          
    127          #ifndef HAL_SLEEP_DEBUG_POWER_MODE
    128          /* set CC2530 power mode; always use PM2 */
    129          #define HAL_SLEEP_PREP_POWER_MODE(mode)     st( SLEEPCMD &= ~PMODE; /* clear mode bits */    \
    130                                                          SLEEPCMD |= mode;   /* set mode bits   */    \
    131                                                          while (!(STLOAD & LDRDY));                   \
    132                                                          halSleepPconValue = PCON_IDLE;               \
    133                                                        )
    134          #define HAL_SLEEP_SET_POWER_MODE()          halSetSleepMode()
    135          #else
    136          /* Debug: don't set power mode, just block until sleep timer interrupt */
    137          #define HAL_SLEEP_PREP_POWER_MODE(mode)     /* nothing */
    138          #define HAL_SLEEP_SET_POWER_MODE()          st( while(halSleepInt == FALSE); \
    139                                                          halSleepInt = FALSE;         \
    140                                                          HAL_DISABLE_INTERRUPTS();    \
    141                                                        )
    142          #endif
